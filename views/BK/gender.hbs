  <div class="content-wrapper">
    

    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>Gender Post and Grid</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">Gender</li>
            </ol>
          </div>
        </div><!-- /.container-fluid -->
    </section>
    <!-- Close Content Header (Page header) -->
    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <!-- left column -->
          <div class="col-md-12">
            <!-- jquery validation -->
            <div class="card card-primary">
              <div class="card-header">
                <h3 class="card-title">Gender Submit</small></h3>
              </div>
              <!-- /.card-header -->
              <!-- form start -->
              {{#if msg}}
              <p class="{{msg_type}} m-3">{{msg}}</p>
              {{/if}}
              <form id="form_val" action="/submit" method="post">
                <div class="card-body">
                  <div class="form-group">
                    <label for="Gcode">Enter Gender Code</label>
                    <input type="text" name="Gcode" class="form-control" id="Gcode" placeholder="Gender Code">
                  </div>
                <div class="form-group">
                    <label for="Gname">Enter Name</label>
                    <input type="text" name="Gname" class="form-control" id="Gname" placeholder="Name">
                </div>

                <div class="form-group">
    <label for="status">Status</label><br>
    <!-- Dropdown button -->
    <div class="btn-group">
        <button type="button" class="btn btn-primary dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false">
            Active
        </button>
        <ul class="dropdown-menu">
            <li><a class="dropdown-item" href="#" onclick="setStatus('Active')">Active</a></li>
            <li><a class="dropdown-item" href="#" onclick="setStatus('Inactive')">Inactive</a></li>
        </ul>
    </div>
    <!-- Hidden input field -->
    <input type="hidden" name="status" id="status" value="Active"> <!-- Default to Active -->
</div>
                  <button type="submit" class="btn btn-primary float-right">Submit</button>
              </form>
            </div>
          </div>
            <!-- /.card -->
            </div>
          <!--/.col (left) -->
          <!-- right column -->
          <!--/.col (right) -->
        </div>
        </div>

        

    </section>
